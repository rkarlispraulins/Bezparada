{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/contact.ts": {
      "runtime": "@vercel/node@3"
    },
    "api/consultations.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}