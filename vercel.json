{
  "version": 2,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "node build.js",
        "outputDirectory": "dist/public"
      }
    },
    {
      "src": "api/contact.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/consultations.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}